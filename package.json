{
  "name": "nextjs-blog-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"babel -w ./db --out-dir dist --extensions .ts,.tsx\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typeorm:build": "babel -w ./db --out-dir dist --extensions .ts,.tsx",
    "m:create": "typeorm migration:create",
    "m:run": "./node_modules/typeorm/cli.js migration:run -d ./dist/data-source.js",
    "m:revert": "typeorm migration:revert -d ./dist/data-source.js",
    "e:create": "typeorm entity:create",
    "db:seed": "node dist/seed.js"
  },
  "dependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/runtime": "^7.27.0",
    "axios": "^1.8.4",
    "github-markdown-css": "^5.8.1",
    "lodash": "^4.17.21",
    "marked": "^15.0.12",
    "md5": "^2.3.0",
    "next": "15.2.4",
    "next-iron-session": "^4.2.0",
    "pg": "^8.14.1",
    "query-string": "^9.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/axios": "^0.14.4",
    "@types/lodash": "^4.17.16",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.13.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
